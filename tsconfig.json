{
	"compilerOptions": {
	  "target": "ESNext",
	  "lib": ["dom", "dom.iterable", "esnext"],
	  "allowJs": true,
	  "skipLibCheck": true,
	  "strict": false,
	  "forceConsistentCasingInFileNames": true,
	  "noEmit": true,
	  "esModuleInterop": true,
	  "module": "esnext",
	  "moduleResolution": "node",
	  "resolveJsonModule": true,
	  "isolatedModules": true,
	  "jsx": "preserve",
	  "baseUrl": ".",
	  "paths": {
		  "#data-transfer-types/*": [
			  "./*"
		  ],
		  "#src/*": [
			  "./src/*"
		  ],
		  
		  "#components/*": [
			  "./src/components/*"
		  ],
		  "#styles/*": [
			  "./src/styles/*"
		  ],
		  "#libs/*": [
			  "./src/libs/*"
		  ],
		  "#types/*": [
			  "./src/types/*"
		  ],

		  "#data/*": [
			  "src/data/*"
		  ],

		  "#commerce/*": [
			  "./src/components/commerce/*"
		  ],
		  "#ui/*": [
			  "./src/components/ui/*"
		  ],
		  "#skeleton/*": [
			  "./src/components/skeleton/*"
		  ],
		  "#services/*": [
			  "./src/services/*"
		  ],
		  "#repositories/*": [
			  "./src/repositories/*"
		  ]
	  }
	},
	"exclude": ["node_modules", ".next", "out"],
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.js"]
  }